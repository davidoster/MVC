<h1>List of Products</h1>
<% products.forEach((product) => { %>
    <%= product.id %>, <%= product.name %>,
    <%= product.description %>,
    <%= product.price %>,
    <%= product.dateOfRelease %>
    <a href='<%= '/products/edit/' + product.id  %>'>Edit</a>&nbsp;<a href='<%= '/products/delete/' + product.id  %>'>Delete</a><br> 
<% })  %>
